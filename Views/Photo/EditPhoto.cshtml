@{
    Layout = null;
}
@using p3.Models
@using System.Linq
@model System.Collections.ArrayList

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>EditEvent</title>
    <link href="/Content/bootst.css" rel="stylesheet" />
    <script src="/Scripts/bootstrap.js"></script>
</head>
<body>
    <div style="color: #ffffff">
        @foreach (Photo e in Model)
{

    <form role="form" action="Edit" method="post" enctype="multipart/form-data">

        <input type="hidden" name="Id" class="form-control" value="@e.Id">
        
        <img src="/img/@e.imgName"> </img>

        <input type="file" name="imgName" value="upload" />


        <button type="submit" class="btn btn-default pull-right">Upload</button>


    </form>


}

    </div>

</body>
</html>
